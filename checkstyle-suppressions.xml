<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC "-//Puppy Crawl//DTD Suppressions 1.1//EN" "http://www.puppycrawl.com/dtds/suppressions_1_1.dtd">
<suppressions>
	<suppress checks="JavadocMethod" files=".*[/\\]test[/\\].*" />
	<suppress checks="JavadocPackage" files=".*[/\\]test[/\\].*" />
	<!-- exclude generated JMH classes from all checks -->
	<suppress checks="[a-zA-Z0-9]*"
		files=".*[/\\]generated-test-sources[/\\].*" />
	<suppress checks="RedundantModifier"
		files="ConstructorUtilsTest" lines="0-99999" />
</suppressions>